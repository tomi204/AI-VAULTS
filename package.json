    "deploy:simple:rootstock": "npx hardhat run scripts/deploy-simple-vault.ts --network rootstock",
    "deploy:simple:rootstock-testnet": "npx hardhat run scripts/deploy-simple-vault.ts --network rootstockTestnet",
    "tokens:simple:flow": "npx hardhat run scripts/get-simple-vault-tokens.ts --network flow",
